{
  "base_dir": "/tmp/crimeflare",
  "base_url": "http://crimeflare.net:82/domains",
  "base_port": 82,
  "curl_timeout": 300,
  "pdo": {
    "host": "127.0.0.1",
    "db": "crimeflare",
    "user": "crimeflare",
    "pass": "MakeSureIamSecure",
    "charset": "utf8",
    "timeout": 120
  },
  "crimeflare": {
    "ipout": {
      "file": "ipout",
      "sql": {
        "table": "crimeflare_ip",
        "fields": {
          "updated_at": "VARCHAR(255)",
          "domain": "VARCHAR(255)",
          "ip_address": "VARCHAR(255)"
        }
      }
    },
    "nsout": {
      "file": "nsout",
      "sql": {
        "table": "crimeflare_ns",
        "fields": {
          "nsone": "VARCHAR(255)",
          "nstwo": "VARCHAR(255)",
          "domain": "VARCHAR(255)"
        }
      }
    },
    "country": {
      "file": "country",
      "sql": {
        "table": "crimeflare_country",
        "fields": {
          "domain": "VARCHAR(255)",
          "ip_address": "VARCHAR(255)",
          "country": "VARCHAR(255)"
          }
      }
    }
  }
}
